{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "no-console": [
      false,
      "log",
      "error",
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ],
    "object-literal-sort-keys": false
  },
  "jsRules": {
    "object-literal-sort-keys": false,
    "no-console": [
      false,
      "log",
      "error",
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ]
  },
  "linterOptions": {
    "exclude": ["config/**/*.js", "node_modules/**/*.ts", "src/lib/*.js"]
  }
}
